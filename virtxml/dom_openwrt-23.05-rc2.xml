<domain type="kvm">
    <name>openwrt-23.05-rc2</name>
    <memory unit="M">512</memory>
    <vcpu placement="static">2</vcpu>
    <os>
        <type arch="x86_64" machine="pc-q35-8.0">hvm</type>
        <boot dev="hd"/>
    </os>
    <cpu>
        <model fallback="allow">qemu64</model>
    </cpu>
    <devices>
        <disk type="volume" device="disk">
            <driver name="qemu" type="qcow2"/>
            <source pool="gat" volume="openwrt.qcow2"/>
            <target dev="vda" bus="virtio"/>
        </disk>
        <interface type="network">
            <mac address="00:ab:ad:1d:eb:00"/>
            <source network="gat-ptp"/>
            <model type="virtio"/>
        </interface>
        <interface type="network">
            <mac address="00:ab:ad:1d:eb:01"/>
            <source network="gat-lan"/>
            <model type="virtio"/>
        </interface>
        <console type="pty">
            <target type="virtio"/>
        </console>
    </devices>
</domain>
